                    {
                      "id": "kommuneplanen.pbya",
                      "type": "Answer",
                      "heading": "%BYA",
                      "value": "pbya"
                    },
                    {
                      "id": "kommuneplanen.bra",
                      "type": "Answer",
                      "heading": "BRA",
                      "value": "bra"
                    },
                    {
                      "id": "kommuneplanen.pbra",
                      "type": "Answer",
                      "heading": "%BRA",
                      "value": "pbra"
                    },
                    {
                      "id": "kommuneplanen.tbra",
                      "type": "Answer",
                      "heading": "T-BRA",
                      "value": "tbra"
                    },
                    {
                      "id": "kommuneplanen.ptu",
                      "type": "Answer",
                      "heading": "%TU",
                      "value": "ptu"
                    },
                    {
                      "id": "kommuneplanen.none",
                      "type": "Answer",
                      "heading": "Ingen av disse",
                      "value": "none"
                    }
                  ]
